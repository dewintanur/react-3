ct'\r\nimport {Jumbotron} from 'react-bootstrap';\r\nrequire('./style.css');\r\n\r\nclass Beranda extends React.Component{\r\n    render (){\r\n        return(\r\n            <div class=\"beranda\">\r\n                \r\n                    <h1> Hallo,</h1>\r\n                    <h3>Selamat Datang</h3>\r\n                \r\n            </div>\r\n        )\r\n    }\r\n} \r\nexport default Beranda;\r\n",["48","49"],"D:\\kelas 11\\PROD RPL\\reactJS\\react3\\src\\Components\\kontak.js",[],"D:\\kelas 11\\PROD RPL\\reactJS\\react3\\src\\Components\\tentangsaya.js",[],"D:\\kelas 11\\PROD RPL\\reactJS\\react3\\src\\Components\\List.js",["50"],"import React, {Component} from \"react\";\r\nimport $ from \"jquery\";\r\nclass List extends React.Component {\r\n    constructor(){\r\n        super();\r\n        this.state = {\r\n            siswa : [\r\n                {nis:\"100\", nama:\"Jeffry\", alamat:\"Surabaya\"},\r\n                {nis:\"101\", nama:\"Udin\", alamat:\"Malang\"},\r\n                {nis:\"102\", nama:\"Ken\", alamat:\"Pasuruan\"},\r\n            ],\r\n            nis: \"\",\r\n            nama: \"\",\r\n            alamat: \"\",\r\n            action: \"\"\r\n        }\r\n    }\r\n    bind = (event) => {  \r\n        this.setState({[event.target.name]: event.target.value});  \r\n        /* fungsi ini digunakan untuk memasukkan data dari elemen input \r\n        ke variable state. \r\n        contoh ketika input nis diisi, maka secara otomatis variabel nis \r\n        pada state bernilai sesuai dengan inputan \r\n        */  \r\n      }  \r\n        \r\n      SaveSiswa = (event) =>{  \r\n        event.preventDefault();  \r\n        // temp digunakan untuk menyimpan sementara  \r\n        // data array siswa  \r\n        let temp = this.state.siswa;  \r\n        \r\n        if (this.state.action === \"insert\") {  \r\n          // temp akan ditambahkan dengan data siswa yang baru  \r\n          // sesuai dengan data yang dimasukkan pada form  \r\n          temp.push({  \r\n            nis: this.state.nis,  \r\n            nama: this.state.nama,  \r\n            alamat: this.state.alamat  \r\n          });  \r\n        } else if (this.state.action === \"update\") {  \r\n          // mencari index data yang akan diubah  \r\n          let index = temp.findIndex(item => item.nis === this.state.nis);  \r\n          // mengubah data array sesuai dengan masukan pada form  \r\n          temp[index].nama = this.state.nama;  \r\n          temp[index].alamat = this.state.alamat;  \r\n        }  \r\n        \r\n        \r\n        // array siswa diupdate dengan nilai data temp  \r\n        this.setState({siswa: temp});  \r\n        \r\n        // menutup modal  \r\n        $(\"#modal\").modal('hide');  \r\n      }  \r\n        \r\n      Add = () => {  \r\n        // mengosongkan nilai nis, nama, dan alamat  \r\n        // pada saat tombol add ditekan  \r\n        this.setState({  \r\n          nis: \"\",  \r\n          nama: \"\",  \r\n          alamat: \"\",  \r\n          action: \"insert\"  \r\n        });  \r\n      }  \r\n        \r\n      Edit = (item) => {  \r\n        this.setState({  \r\n          nis: item.nis,  \r\n          nama: item.nama,  \r\n          alamat: item.alamat,  \r\n          action: \"update\"  \r\n        });  \r\n      }  \r\n        \r\n      Drop = (index) => {  \r\n        // temp digunakan untuk menyimpan sementara  \r\n        // data array siswa  \r\n        let temp = this.state.siswa;  \r\n        \r\n        // menghapus data pada index yang dihapus  \r\n        temp.splice(index,1);  \r\n        \r\n        // array siswa diupdate dengan nilai data temp  \r\n        this.setState({siswa: temp});  \r\n      }\r\n    render(){\r\n        return(\r\n            <div className=\"container\">\r\n                {/* generate list */}\r\n                <ul className=\"list-group\">\r\n                    {this.state.siswa.map((item, index)=> {\r\n                        return (\r\n                            <li className=\"list-group-item\" key={index}>\r\n                                <h5 className=\"text-info\">{item.nama}</h5>\r\n                                <h6>NIS: {item.nis}</h6>\r\n                                <h6>Alamat: {item.alamat}</h6>\r\n\r\n                                <button className=\"btn btn-sm btn-primary m-1\" \r\n                                onClick={()=> this.Edit(item)}\r\n                                data-toggle=\"modal\" data-target=\"#modal\">\r\n                                    Edit\r\n                                </button>\r\n                                <button className=\"btn btn-sm btn-danger m-1\" \r\n                                onClick={()=> this.Drop(index)}>\r\n                                Hapus\r\n                                </button>\r\n                            </li>\r\n                        );\r\n                    })}\r\n                </ul>\r\n                            <button className=\"btn btn-sm btn-success m-3\" onClick={this.Add}\r\n                                data-toggle=\"modal\" data-target=\"#modal\">\r\n                                    Tambah Data\r\n                            </button>\r\n\r\n                                { /*elemen for modal */}\r\n                            <div className=\"modal fade\" id=\"modal\">\r\n                                <div className=\"modal-dialog\">\r\n                                    <div className=\"modal-content\">\r\n                                        <div className=\"modal-header bg-success text-white\">\r\n                                            <h5>Form Siswa</h5>\r\n                                        </div>\r\n                                            <form onSubmit={this.SaveSiswa}>\r\n                                                <div className=\"modal-body\">\r\n                                                    NIS\r\n                                                    <input type=\"text\" name=\"nis\" className=\"form-control\" onChange={this.bind}\r\n                                                    value={this.state.nis} />\r\n                                                    Nama\r\n                                                    <input type=\"text\" name=\"nama\" className=\"form-control\" onChange={this.bind}\r\n                                                    value={this.state.nama} />\r\n                                                    Alamat\r\n                                                    <input type=\"text\" name=\"alamat\" className=\"form-control\" onChange={this.bind}\r\n                                                    value={this.state.alamat} />\r\n                                                    </div>\r\n                                                    <div className=\"modal-footer\">\r\n                                                       <button type=\"submit\" className=\"btn btn-primary\">\r\n                                                           Simpan\r\n                                                        </button>\r\n                                                    </div> \r\n                                            </form>\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n            </div>\r\n        );\r\n    }\r\n}\r\nexport default List;\r\n\r\n","D:\\kelas 11\\PROD RPL\\reactJS\\react3\\src\\Components\\Footer.js",[],{"ruleId":"51","severity":1,"message":"52","line":3,"column":9,"nodeType":"53","messageId":"54","endLine":3,"endColumn":13},{"ruleId":"51","severity":1,"message":"55","line":4,"column":13,"nodeType":"53","messageId":"54","endLine":4,"endColumn":27},{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","replacedBy":"59"},{"ruleId":"51","severity":1,"message":"60","line":2,"column":9,"nodeType":"53","messageId":"54","endLine":2,"endColumn":18},{"ruleId":"56","replace